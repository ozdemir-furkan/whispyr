{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Llm": {
    "Retry": {
      "MaxAttempts": 5,
      "BaseDelayMs": 200,
      "JitterMs": 200,
      "RespectRetryAfter": true,
      "RetryOn5xx": true
    }
  }
  
}
